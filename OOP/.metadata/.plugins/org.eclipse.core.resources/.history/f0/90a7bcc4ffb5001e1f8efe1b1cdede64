package parent;

public class Parent {
	public static String name;
	
	public Parent(String name) {
		Parent.name = name;
	}

	public static String getName() {
		return name;
	}
	
	public static class Inner {
		private String name;

		public Inner(String name) {
			this.name = name;
		}
		
		public String getName() {
			return name;
		}

		public void setName(String name) {
			this.name = name;
		}
	}
}
