package parent;

public class Parent {
	public static final String NAME = "Agim";
	
	public static String getName() {
		return NAME;
	}
	
	public static class Inner {
		private String name;

		public Inner(String name) {
			this.name = name;
		}
		
		public String getName() {
			return name;
		}

		public void setName(String name) {
			this.name = name;
		}
	}
}
